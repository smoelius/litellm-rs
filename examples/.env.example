# LiteLLM Configuration Example
# Copy this file to .env and add your API keys

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_BASE=https://api.openai.com/v1  # Optional: custom endpoint
OPENAI_ORG_ID=your-org-id  # Optional

# Anthropic/Claude Configuration
ANTHROPIC_API_KEY=your-anthropic-api-key-here
ANTHROPIC_API_BASE=https://api.anthropic.com  # Optional

# Azure OpenAI Configuration
AZURE_API_KEY=your-azure-api-key
AZURE_API_BASE=https://your-resource.openai.azure.com
AZURE_API_VERSION=2024-02-15-preview
AZURE_DEPLOYMENT_NAME=your-deployment-name

# Google AI Studio (Gemini)
GOOGLE_API_KEY=your-google-api-key
GOOGLE_PROJECT_ID=your-gcp-project-id
GOOGLE_REGION=us-central1

# Cohere Configuration
COHERE_API_KEY=your-cohere-api-key

# Replicate Configuration
REPLICATE_API_KEY=your-replicate-api-key

# Hugging Face Configuration
HUGGINGFACE_API_KEY=your-huggingface-api-key

# Together AI Configuration
TOGETHERAI_API_KEY=your-together-api-key

# OpenRouter Configuration
OPENROUTER_API_KEY=your-openrouter-api-key

# Perplexity Configuration
PERPLEXITY_API_KEY=your-perplexity-api-key

# Mistral Configuration
MISTRAL_API_KEY=your-mistral-api-key

# Default Model Settings
DEFAULT_MODEL=gpt-3.5-turbo
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=1000

# Gateway Configuration (if using gateway mode)
GATEWAY_HOST=0.0.0.0
GATEWAY_PORT=8080
RUST_LOG=info

# Database (optional)
DATABASE_URL=postgresql://user:password@localhost:5432/litellm

# Redis Cache (optional)
REDIS_URL=redis://localhost:6379
CACHE_ENABLED=true
CACHE_TTL=3600

# Rate Limiting
GLOBAL_RPM=1000
GLOBAL_TPM=50000